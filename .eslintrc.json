{
      "env": {
            "es6": true,
            "cypress/globals": true
      },
      "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended",
            "plugin:mocha/recommended"
      ],
      "plugins": ["mocha", "cypress"],
      "globals": {
            "describe": true,
            "it": true
      },
      "rules": {
            "mocha/no-mocha-arrows": "off",
            "prettier/prettier": ["error", { "endOfLine": "auto" }]
      }
}
